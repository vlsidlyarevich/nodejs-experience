{
  "extends": "airbnb",
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "indent": [
      "error",
      4
    ],
    "strict": [
      "error",
      "global"
    ],
    "comma-dangle": [
      "error",
      "never"
    ]
  }
}